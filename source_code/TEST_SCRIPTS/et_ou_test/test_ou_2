rm -rf toto truc

ls | cat -e || echo OK
ls > toto || echo OK
ls -a >> toto || echo OK
ls -z || echo OK | cat -e

echo OK | cat -z || echo "OKOK"
ls -z || echo OK | cat -z || echo "OKOK"
ls -z || echo OK | cat -e || echo "OKOK" > truc || echo "ENCORE OK" " YOLO "
ls -z || echo OK | cat -z || echoz "OKOK" > truc || echo "ENCORE OK" " YOLO "

cd / || pwd
cd existepas || pwd
chmod 000 existepas || cd existepas || echo truc
ls -z || ls || ls -z ; pwd

rm -rf toto truc